extends extends/layout.jade

block html
	- var pageTitle = 'Arzte | Ärztehaus Grein – Dr. Huemer'
	//- - var screen = true
	- var bodyclass = 'inside page-color-beige'

block content
	main(role="main").setw
		h1 Arzte

		section.sec
			.sec-head
				h2 Arzt für Allgemeinmedizin
				.icon.i-clinic

			.sec-body
				.card
					.card-left
						img.avatar(alt="Dr. Huemer Thomas foto", src="assets/i/tmp/Dr-Huemer-Tomas.png")
					.card-content
						.d-t.fullw
							.card-content-left.text
								.d-t.fullw.d-t-card
									.d-t-row
										.d-t-cell Name:
										.d-t-cell
											b Dr. Thomas Huemer
									.d-t-row
										.d-t-cell Kassen:
										.d-t-cell
											.line-obj
												a(href="#") GKK
												a(href="#") BVA
												a(href="#") VA
												a(href="#") SVA
												a(href="#") KFA
												a(href="#") SVB
									.d-t-row
										.d-t-cell Ordinationszeiten:
										.d-t-cell
											p.schedule
												span.schedule-day Mo
												span.schedule-hours 08:00 - 12:00   /  16:00 - 18:00
											p.schedule
												span.schedule-day Di
												span.schedule-hours 08:00 - 12:00
											p.schedule
												span.schedule-day Sa
												span.schedule-hours nur nach telefonischer Vereinbarung
							.card-content-right
								a.card-print(href="#"): .i-print
								a.card-email(href="#"): .i-email



		section.sec
			.sec-head
				h2 Klinische- und Gesundheitspsychologin, Biofeedbacktherapeutin
