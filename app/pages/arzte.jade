					.card-content
						.d-t.fullw
							.card-content-left.text
								.d-t.fullw.d-t-card
									.d-t-row
										.d-t-cell Name:
										.d-t-cell
											b Mag. Silke Maximoff
									.d-t-row
										.d-t-cell Kassen:
										.d-t-cell Wahlärztin
									.d-t-row
										.d-t-cell Telefon:
										.d-t-cell 0664 / 57 08 736
									.d-t-row
										.d-t-cell Ordinationszeiten:
										.d-t-cell nach telefonischer Vereinbarung
									.d-t-row
										.d-t-cell E-Mail-adresse:
										.d-t-cell psychologie@maximoff.net
									.d-t-row
										.d-t-cell Website:
										.d-t-cell maximoff.net
							.card-content-right
								a.card-print(href="#"): .i-print
								a.card-email(href="#"): .i-email

			
